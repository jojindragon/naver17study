package day1212;

public class Ex2Operator {

	public static void main(String[] args) {
		// 산술 연산자
		int v1 = 11;
		int v2 = 4;
		
		System.out.println(v1+v2);
		System.out.println(v1-v2);
		System.out.println(v1*v2);
		System.out.println(v1/v2);
		System.out.println((double)v1/v2);
		System.out.println(v1%v2);
		

	}

}
