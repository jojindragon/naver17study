package day1211;

/*
  * 이름 : Candy님
  * 나이 : 32세
  * 키 : 163.8Cm
  * 몸무게 : 56.7Kg
  * 혈액형 : B형 
 */

public class Ex7DataType {

	public static void main(String[] args) {
		// printf로 위와 같이 출력 해보기
		String name = "Candy";
		int age = 32;
		float height = 163.8f;
		double weight = 56.7;
		char blood = 'B';
				
		System.out.printf("* 이름  : %10s님\n", name);
		System.out.printf("* 나이  : %5d세\n", age);
		System.out.printf("* 키  : %5.1fCm\n", height);
		System.out.printf("* 몸무게  : %5.1fKg\n", weight);
		System.out.printf("* 혈액형  : %3c형\n", blood);
		
		
	}

}
