package day1212;

public class Ex5Operator {

	public static void main(String[] args) {
		// 삼항 연산자 == 조건연산자 ==> 조건식1 ? true 문장 : false 문장
		// 복수 조건 ==> 조건식1 ? true 문장 : 조건식2 ? true 문장 : 조건식3 ? true 문장 : false 문장
		int x, y, max;
		x = 5;
		y = 13;
		max = x > y ? x : y;
		System.out.println("max="+max);

	}

}
