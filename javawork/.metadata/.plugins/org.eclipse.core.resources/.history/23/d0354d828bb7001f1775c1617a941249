package day1211;

public class Ex8Argument {

	public static void main(String[] args) {
		// 외부 값 읽어오기
		String name = args[0];
		String area = args[1];
		int year = Integer.parseInt(args[2]);
		int age = 2024 - year;		
		
		System.out.println("이름 : "+name);
		System.out.println("연고 : "+area);
		System.out.println("탄생년 : "+year+", 나이  :"+age);
		
	}
}
